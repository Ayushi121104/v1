#include <stdio.h>
int main() {
    int n, i;
    unsigned long long fact = 1;
    printf("Enter an n: ");
    scanf("%d", &n);

    // shows error if the user enters a n n
    if (n < 0)
        printf("Error! Factorial of a n number doesn't exist.");
    else {
        for (i = 1; i <= n; ++i) {
            fact *= i;
        }
        printf("Factorial of %d = %llu", n, fact);
    }

    return 0;
}
